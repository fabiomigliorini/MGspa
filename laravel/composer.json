{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "dompdf/dompdf": "^2.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "geekcom/validator-docs": "^3.10",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^8.12",
        "laravel/helpers": "^1.6",
        "laravel/passport": "^10.1",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.2",
        "nfephp-org/sped-common": "^5.1",
        "nfephp-org/sped-da": "^1.1",
        "nfephp-org/sped-ibpt": "^2.0",
        "nfephp-org/sped-mdfe": "^4.1",
        "nfephp-org/sped-nfe": "^5.1",
        "openboleto/openboleto": "dev-master",
        "phpoffice/phpspreadsheet": "^1.28",
        "picqer/php-barcode-generator": "^2.4",
        "quilhasoft/jasperphp": "dev-master",
        "quilhasoft/jasperphp-openboleto": "dev-master"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true,
            "symfony/thanks": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Mg/Helpers/MgHelpers.php"
        ],
        "classmap": [
            
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Mg\\": "app/Mg"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
